package com.nt.beans;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.beans.factory.annotation.Qualifier;

public class Flipkart {
	
	  @Autowired
	  
	  @Qualifier("dhl")
	 
	private Courier c ;
	
	
	
	public Flipkart()
	{
		System.out.println("Flipkart 0-param constructor");
	}
	
	//@Autowired at constructor
	
	  @Autowired
	  
	 public Flipkart(@Qualifier("bluedart") Courier c) { this.c=c;
	  System.out.println("flipkart 1-param constructor"); }
	 
	
	//@Autowired at setter-method
	/*
	 * @Autowired
	 * 
	 * @Qualifier("dhl") public void setCourier(Courier c) { this.c=c;
	 * System.out.println("flipkart setter method"); }
	 */
	
	
	//@Autowiring at arbitary method level
	@Autowired
	@Qualifier("dtdc")
	public void fun(Courier c)
	{
		this.c=c;
		System.out.println("arbitary method");
	}
	
	
	
	
	
	public void callcouriercompanies()
	{
		System.out.println(c.delivery());
	}

}
