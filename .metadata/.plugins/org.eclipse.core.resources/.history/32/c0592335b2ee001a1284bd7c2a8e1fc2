package com.calculateintrest.bo;

public class CustomerBo {
private int cno;
private String cname;
private String cadd;
private float cprincipalamnt;
private float cintrestamnt;
public int getCno() {
	return cno;
}
public void setCno(int cno) {
	this.cno = cno;
}
public String getCname() {
	return cname;
}
public void setCname(String cname) {
	this.cname = cname;
}
public String getCadd() {
	return cadd;
}
public void setCadd(String cadd) {
	this.cadd = cadd;
}
public float getCprincipalamnt() {
	return cprincipalamnt;
}
public void setCprincipalamnt(float cprincipalamnt) {
	this.cprincipalamnt = cprincipalamnt;
}
public float getCintrestamnt() {
	return cintrestamnt;
}
public void setCintrestamnt(float cintrestamnt) {
	this.cintrestamnt = cintrestamnt;
}

}
